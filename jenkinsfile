pipeline {
    agent any

     tools { 
        maven 'Maven 3.3.9' 
        jdk 'jdk8' 
    }
    stages {
        stage('init'){
          steps {
                 echo "PATH = ${PATH}"              
                 echo "M2_HOME = ${M2_HOME}"
             }
        }
     	stage('checkout') {
            steps {
                 checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/vikashsaini01/Practice_Java.git']]])
                  echo 'checkedout..'
            }
        }
        stage('build') {
            steps {
                sh "mvn clean package"
                echo 'Building..'
            }
        }
         stage('publish') {
            steps {
                echo 'Building..'
            }
        }        
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
         stage('verify') {
            steps {
                echo 'Building..'
            }
        }
    }
}
